/ {
	mipi_en: mipi-en {
                compatible = "regulator-fixed";
                regulator-name = "mipi_en";
                gpio = <&gpio3 RK_PC6 GPIO_ACTIVE_HIGH>;
                regulator-always-on;
                enable-active-high;
                pinctrl-names = "default";
                pinctrl-0 = <&mipi_power_en>;
        };

};

