&dsi1_in_vp2 {
	status = "disabled";
};

&dsi1_in_vp3 {
	status = "okay";
};

&backlight {
	status = "okay";
	pwms = <&pwm1 0 25000 0>;
};

&pwm1 {
	status = "okay";
};

&mipi_dcphy1 {
	status = "okay";
};

&route_dsi1 {
	status = "okay";
	connect = <&vp3_out_dsi1>;
};

&i2c5 {
	status = "okay";

	gt1x: gt1x@14 {
		compatible = "goodix,gt1x";
		reg = <0x14>;
		pinctrl-names = "default";
		//power-supply = <&vcc3v3_lcd0_n>;
		pinctrl-0 = <&touch_gpio>;
		//goodix,enable-gpio = <&gpio3 RK_PB1 GPIO_ACTIVE_HIGH>;
		goodix,rst-gpio = <&gpio3 RK_PC1 GPIO_ACTIVE_HIGH>;
		goodix,irq-gpio = <&gpio3 RK_PC0 IRQ_TYPE_LEVEL_LOW>;
	};
};

&pinctrl {
	mipi_pwren {
		mipi_power_en: mipi_power_en {
			rockchip,pins = <3 RK_PC6 RK_FUNC_GPIO &pcfg_output_high>;
		};
	};

	touch {
		touch_gpio: touch-gpio {
			rockchip,pins =
				<3 RK_PC1 RK_FUNC_GPIO &pcfg_pull_up>,
				<3 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};
